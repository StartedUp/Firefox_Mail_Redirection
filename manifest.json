{

  "manifest_version": 2,
  "name": "Mail Redirection",
  "version": "0.1",
  
  "description": "Mail Redirection Tool",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/borderify",
  
  "icons": {
    "48": "icons/message-48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Jquery.js","myScript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
],